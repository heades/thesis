binaryWords, w, v :: 'BW_' ::= 
  | em      :: :: empty
  {{tex \text{e}_0 }}
  | -       :: :: Var
  | w [] v  :: :: box  
  {{tex [[w]]\,\Box\,[[v]] }}
  | ( w )   :: S :: parens
  | w b     :: M :: Subst
  {{tex [[w]]_{[[b]]} }}

cats :: 'Cats_' ::=
  | Nat :: :: Nat
  {{tex \mathit{Nat} }}
  | Set :: :: Set
  {{tex \mathit{Set} }}

streams :: 'Streams_' ::=
  | 1 : : streams :: :: StreamOnes
  | 2 : : streams :: :: StreamTwos
  | 1 :: :: One
  | 2 :: :: Two

formula :: 'formula_' ::=
  | ( formula )  ::  :: parens
  | head :: :: head
  {{tex \mathsf{head} }}
  | next :: :: next
  {{tex \mathsf{next} }}
  | double :: :: double
  {{tex \mathsf{double} }}
  | plus :: :: plus
  {{tex \mathsf{plus} }}

term, t :: 'term_' ::=
  | \ xm . t :: :: Lam
  {{tex \lambda [[xm]] . [[t]] }}
  | obj :: :: obj
  | morph :: :: morph
  | t ( t' ) :: :: App
  | ( t ) :: :: parens
  | h( t ) :: :: hparens
  {{tex [[t]] }}





